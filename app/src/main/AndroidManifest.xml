<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paco.qapplaapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/qapplalog"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Registro.RegistryActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registro.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Prueba.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BorrarActivity"
            android:label="@string/title_activity_borrar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Prueba.BorrarActivity"
            android:label="@string/title_activity_borrar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Prueba.Aver"
            android:label="@string/title_activity_aver"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Prueba.vea"
            android:label="@string/title_activity_vea"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registro.WhatUPlay"
            android:label="@string/title_activity_what_uplay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registro.WelcomeActivity"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Registro.GamerTagActivity"
            android:label="@string/title_activity_gamer_tag"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>